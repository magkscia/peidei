	local DetecteAlgema = Button_Protection:Clone()
	DetecteAlgema.Name = "toggle2"
	DetecteAlgema.Titulo.Text = "Detecter Algema"
	DetecteAlgema.Visible = true
	DetecteAlgema.Parent = Button_Protection.Parent
	local maxDistanciaScript1 = 50
	local playerAntAlgema = game.Players.LocalPlayer
	local ferramentasAlvoScript1 = {"Algema", "algema", "Algemas", "algemas", "Algem", "algem"}

	local function verificarJogadoresProximosScript1()
		-- Verifica se o jogador local e seu personagem existem
		local player = game.Players.LocalPlayer
		local character = player.Character
		if character then
			-- Verifica se o personagem tem a parte HumanoidRootPart
			local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
			if humanoidRootPart then
				-- Obtém a posição do jogador local
				local posicaoJogador = humanoidRootPart.Position

				-- Itera sobre os outros jogadores no jogo
				for _, otherPlayer in pairs(game.Players:GetPlayers()) do
					-- Verifica se o outro jogador não é o jogador local
					if otherPlayer ~= player then
						-- Verifica se o outro jogador tem um personagem e a parte HumanoidRootPart
						local otherCharacter = otherPlayer.Character
						local posicaoOutroJogador = otherCharacter and otherCharacter:FindFirstChild("HumanoidRootPart") and otherCharacter.HumanoidRootPart.Position
						if posicaoOutroJogador then
							-- Calcula a distância entre os personagens
							local distancia = (posicaoJogador - posicaoOutroJogador).Magnitude
							if distancia <= maxDistanciaScript1 then
								-- Verifica se o outro jogador está segurando uma ferramenta de algema
								local ferramentaNaMao = otherCharacter:FindFirstChildOfClass("Tool")
								if ferramentaNaMao then
									local nomeFerramenta = ferramentaNaMao.Name:lower()
									for _, nomeAlvo in ipairs(ferramentasAlvoScript1) do
										if nomeFerramenta == nomeAlvo:lower() then
											-- Teleporta o jogador local para uma posição aleatória e exibe uma notificação
											game:GetService("StarterGui"):SetCore("SendNotification", {
												Title = "Protection V1",
												Text = "Ant Algema \n By dreck_.",
												Icon = "http://www.roblox.com/asset/?id=11755544304", 
												Duration = 15,
												Button1 = "Fechar",
											})

											local novaPosicao = Vector3.new(
												math.random(-500, 500),
												5, -- Altura
												math.random(-500, 500)
											)

											character:SetPrimaryPartCFrame(CFrame.new(novaPosicao))
											break
										end
									end
								end
							end
						end
					end
				end
			end
		end
	end


	local nomesFerramentasAlvoScript2 = {"Algema", "algema", "Algemas", "algemas", "Algem", "algem"}


	local function possuiAlgemaScript2()
		local backpack = playerAntAlgema.Backpack
		local character = playerAntAlgema.Character

		-- Verifica na mochila do jogador
		for _, item in pairs(backpack:GetChildren()) do
			if item:IsA("Tool") and table.find(nomesFerramentasAlvoScript2, item.Name) then
				return true
			end
		end

		-- Verifica no personagem do jogador
		local characterTools = character:FindFirstChildOfClass("Backpack")
		if characterTools then
			for _, item in pairs(characterTools:GetChildren()) do
				if item:IsA("Tool") and table.find(nomesFerramentasAlvoScript2, item.Name) then
					return true
				end
			end
		end

		return false
	end




	local function verificarJogadoresProximosScript2()
		local maxDistanciaScript2 = 100
		-- Verifica se o jogador local e seu personagem existem
		local player = game.Players.LocalPlayer
		local character = player.Character
		if character then
			-- Verifica se o personagem tem a parte HumanoidRootPart
			local humanoidRootPart = character:FindFirstChild("HumanoidRootPart")
			if humanoidRootPart then
				-- Itera sobre os outros jogadores no jogo
				for _, otherPlayer in pairs(game.Players:GetPlayers()) do
					-- Verifica se o outro jogador não é o jogador local
					if otherPlayer ~= player then
						-- Verifica se o outro jogador tem um personagem e a parte HumanoidRootPart
						local otherCharacter = otherPlayer.Character
						local otherHumanoidRootPart = otherCharacter and otherCharacter:FindFirstChild("HumanoidRootPart")
						if otherHumanoidRootPart then
							-- Calcula a distância entre os personagens
							local distancia = (humanoidRootPart.Position - otherHumanoidRootPart.Position).Magnitude
							if distancia <= maxDistanciaScript2 then
								-- Verifica se o outro jogador possui uma algema
								if possuiAlgemaScript2() then
									-- Exibe uma notificação
									game.StarterGui:SetCore("SendNotification", {
										Title = otherPlayer.Name,
										Text = "ANTI ALGEMA - DETECTAMOS ALGEMA INVENTARIO DO " .. otherPlayer.Name,
										Icon = otherPlayer.Character.Head,
										Duration = 0.7,
									})
								end
							end
						end
					end
				end
			end
		end
	end
	
	
	
