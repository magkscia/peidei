		end

		print(tostring(#dropsSeat))

		if #dropsSeat > 0 then
			local jogadorPos = player.Character and player.Character:FindFirstChild("HumanoidRootPart") and player.Character.HumanoidRootPart.Position

			if jogadorPos then
				local filteredSeats = {}

				-- Filtra assentos com base na distância
				for _, assento in pairs(dropsSeat) do
					local distancia = (assento.Position - jogadorPos).Magnitude
					if distancia > 50 then
						table.insert(filteredSeats, assento)
					end
				end

				if #filteredSeats > 0 then
					local randomDrop = filteredSeats[math.random(1, #filteredSeats)]
					player.Character:SetPrimaryPartCFrame(randomDrop.CFrame)
				else
					warn("Não há gotas disponíveis para teleportar fora do alcance de 100 metros.")
					game.StarterGui:SetCore("SendNotification", {
						Title = "Erro",
						Text = "Tente novamente!",
						Duration = 3,
					})
				end
			end
		else
			warn("Não há gotas disponíveis para teleportar.")
			game.StarterGui:SetCore("SendNotification", {
				Title = "Erro",
				Text = "Não há carros compatíveis",
				Duration = 3,
			})
		end



		wait(3)
		if humanoid.SeatPart then
			local car = humanoid.SeatPart.Parent 

			if car then
				--local customPosition = Vector3.new(0, 0, 0) -- Posição personalizada
				local size = car:GetExtentsSize()
				car:PivotTo(CFrame.new(customPosition + Vector3.new(0, size.Y/2, 0)))
			end
		end
		wait(0.5)


		if customPosition then
			local humanoid = player.Character and player.Character:FindFirstChildOfClass("Humanoid")
			if humanoid then
				humanoid.Parent:SetPrimaryPartCFrame(CFrame.new(customPosition))
			end
		else
			print("Nenhuma posição marcada. Clique no botão 'Marcar' primeiro.")
		end

	end)
	
